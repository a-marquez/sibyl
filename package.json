{
  "name": "sibyl",
  "version": "0.0.1",
  "description": "A modern boilerplate for esri map web applications.",
  "main": "index.js",
  "scripts": {
    "start": "bower install & gulp watch serve",
    "dist": "bower install && NODE_ENV=production gulp dist && r.js -o build.js",
    "deploy-gh-pages": "git subtree push --prefix dist origin gh-pages",
    "version": "npm run dist && git add dist",
    "postversion": "npm run deploy-gh-pages",
    "install": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-marquez/sibyl.git"
  },
  "author": "Albert Marquez",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/a-marquez/sibyl/issues"
  },
  "homepage": "https://github.com/a-marquez/sibyl#readme",
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bower": "^1.7.2",
    "browser-sync": "^2.11.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.1",
    "gulp-jade": "^1.1.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.2.0",
    "gulp-umd": "^0.2.0",
    "node-notifier": "^4.4.0",
    "requirejs": "^2.1.22"
  }
}
